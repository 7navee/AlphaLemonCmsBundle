<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AlUser" table="al_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="roleId" type="integer" column="role_id" nullable="false"/>
    <field name="username" type="string" column="username" length="128" nullable="false"/>
    <field name="password" type="string" column="password" length="128" nullable="false"/>
    <field name="salt" type="string" column="salt" length="128" nullable="false"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="ip" type="string" column="ip" length="15" nullable="true"/>
    <field name="createdAt" type="date" column="created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
